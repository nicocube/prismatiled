{
  "name": "prismatiled",
  "version": "0.0.0",
  "description": "A non-realistic but really simple tiled map library",
  "main": "index.js",
  "scripts": {
    "pretest": "npm run lint",
    "test": "tape test/**/*.spec.js |tap-summary --no-progress",
    "autotest": "nodemon -e js,yml -x 'tape test/**/*.spec.js |tap-summary --no-progress'",
    "autolinttest": "nodemon -e js,yml -x 'npm run lint && tape test/**/*.spec.js |tap-summary --no-progress'",
    "coverage": "nyc tape test/**/*.spec.js && nyc report --reporter=html",
    "lint": "eslint index.js lib/ test/",
    "autolint": "nodemon -x eslint index.js lib/ test/",
    "preversion": "npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nicocube/prismatiled.git"
  },
  "keywords": [
    "tile",
    "map"
  ],
  "author": "Nicolas Lochet",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/nicocube/prismatiled/issues"
  },
  "homepage": "https://github.com/nicocube/prismatiled#readme",
  "devDependencies": {
    "eslint": "^5.12.0",
    "nodemon": "^1.18.9",
    "nyc": "^13.1.0",
    "tap-summary": "^4.0.0",
    "tape": "^4.9.2"
  },
  "dependencies": {
    "seedrandom": "^2.4.4"
  }
}
